   ],
   "source": [
    "let fib_seq n =\n",
    "  let rec aux i a b acc =\n",
    "    if i = n then List.rev acc\n",
    "    else aux (i + 1) b (a + b) (a :: acc)\n",
    "  in\n",
    "  if n <= 0 then []\n",
    "  else aux 0 0 1 [];;\n",
    "\n",
    "fib_seq 5;;"
   ]
  }
